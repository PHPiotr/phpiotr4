{
  "name": "phpiotr4",
  "version": "1.0.0",
  "engines": {
    "node": "9.5.0",
    "npm": "5.6.0"
  },
  "description": "Reporting app for a frequent flyer",
  "main": "bin/www",
  "scripts": {
    "start": "nf start",
    "start-prod": "NODE_ENV=production nf start",
    "lint": "eslint --fix -c .eslintrc.json .",
    "lint-watch": "node node_modules/eslint-watch/bin/esw -w --fix",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "test": "NODE_ENV=production nyc --reporter=html --reporter=text mocha './app/tests/**/*.js' --require babel-core/register",
    "test-watch": "npm run tests -- --watch",
    "build": "NODE_ENV=production webpack -p --config ./webpack/client/webpack.client.prod.js --progress --colors",
    "heroku-postbuild": "./node_modules/.bin/webpack -p --config ./webpack/client/webpack.client.prod.js --progress"
  },
  "keywords": [
    "jwt",
    "webpack",
    "react"
  ],
  "author": "Piotr Kowalski <piet.kowalski@gmail.com>",
