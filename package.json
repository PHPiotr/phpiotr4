{
  "name": "phpiotr4",
  "version": "1.0.0",
  "engines": {
    "node": "7.4.0"
  },
  "description": "Reporting app for a frequent flyer",
  "main": "bin/www",
  "scripts": {
    "test": "mocha './app/tests/**/*.js' --compilers js:babel-core/register",
    "start": "node bin/www",
    "build": "NODE_ENV=production webpack --config ./webpack.config.prod.js --progress --colors",
    "dev-server": "better-npm-run dev-server",
    "prod-server": "better-npm-run prod-server",
    "heroku-postbuild": "./node_modules/.bin/webpack -p --config ./webpack.config.prod.js --progress"
  },
  "betterScripts": {
    "dev-server": {
      "command": "node bin/www",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "prod-server": {
      "command": "node bin/www",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production",
        "PORT": 3000
      }
    }
  },
  "keywords": [
    "jwt",
    "webpack",
    "react"
  ],
  "author": "Piotr Kowalski <piet.kowalski@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.13.0",
    "better-npm-run": "0.0.15",
    "bootstrap-css": "^3.3.7",
    "cookie-monster": "^0.2.1",
    "css-loader": "^0.28.4",
    "debug": "^2.6.8",
    "es6-promise": "^4.1.0",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.11.2",
    "history": "^3.3.0",
    "hoist-non-react-statics": "^1.2.0",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.3.0",
    "moment": "^2.17.1",
    "mongodb": "^2.2.11",
    "mongoose": "^4.6.8",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "prop-types": "15.5.10",
    "react": "15.6.0",
    "react-css-modules": "^4.1.0",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "15.6.0",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-jwt-store": "^2.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.5",
    "react-transition-group": "1.2.0",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "serve-static": "^1.12.3",
    "socket.io-client": "^1.7.2",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^2.3.1",
    "webpack-manifest-plugin": "^1.1.0"
  },
