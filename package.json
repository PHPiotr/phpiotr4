{
  "name": "phpiotr4",
  "version": "1.0.0",
  "engines": {
    "node": "8.1.3",
    "npm": "5.1.0"
  },
  "description": "Reporting app for a frequent flyer",
  "main": "bin/www",
  "scripts": {
    "start": "nf start",
    "test": "better-npm-run test",
    "build": "better-npm-run build",
    "dev-server": "better-npm-run dev-server",
    "prod-server": "better-npm-run prod-server",
    "heroku-postbuild": "./node_modules/.bin/webpack -p --config ./webpack.config.prod.js --progress"
  },
  "betterScripts": {
    "dev-server": {
      "command": "node bin/www",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "development",
        "PORT": 3000
      }
    },
    "prod-server": {
      "command": "node bin/www",
      "env": {
        "NODE_PATH": ".",
        "NODE_ENV": "production",
        "PORT": 3000
      }
    },
    "test": {
      "command": "mocha './app/tests/**/*.js' --compilers js:babel-core/register",
      "env": {
        "NODE_ENV": "production"
      }
    },
    "build": {
      "command": "webpack -p --config ./webpack.config.prod.js --progress --colors",
      "env": {
        "NODE_ENV": "production"
      }
    }
  },
  "keywords": [
    "jwt",
    "webpack",
    "react"
  ],
  "author": "Piotr Kowalski <piet.kowalski@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PHPiotr/phpiotr4.git"
  },
  "license": "MIT",
  "dependencies": {
    "better-npm-run": "^0.0.15",
    "bootstrap-css": "^4.0.0-alpha.5",
    "cookie-monster": "^0.2.1",
    "debug": "^2.6.8",
    "es6-promise": "^4.1.0",
    "express": "^4.15.3",
    "history": "^4.6.3",
    "hoist-non-react-statics": "^2.0.0",
    "isomorphic-fetch": "^2.2.1",
    "jsonwebtoken": "^7.3.0",
    "moment": "^2.17.1",
    "mongodb": "^2.2.11",
    "mongoose": "^4.6.8",
    "prop-types": "^15.5.10",
    "react": "^15.6.1",
    "react-css-modules": "^4.5.2",
    "react-dnd": "^2.1.4",
    "react-dnd-html5-backend": "^2.1.2",
    "react-dom": "^15.6.1",
    "react-hot-loader": "^3.0.0-beta.7",
    "react-jwt-store": "^2.0.0",
    "react-redux": "^5.0.3",
    "react-router": "^3.0.5",
    "redux": "^3.6.0",
    "redux-logger": "^3.0.1",
    "redux-thunk": "^2.2.0",
    "serve-favicon": "^2.4.3",
    "serve-static": "^1.12.3",
    "socket.io-client": "^2.0.3"
  },
  "devDependencies": {
    "babel-core": "^6.24.0",
    "babel-loader": "^7.1.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-polyfill": "^6.22.0",
    "babel-preset-env": "^1.2.2",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.23.0",
    "babel-preset-stage-2": "^6.13.0",
    "css-loader": "^0.28.4",
    "cssnano": "3.10.0",
    "deep-freeze": "^0.0.1",
    "es6-promise-promise": "^1.0.0",
    "eslint": "^4.1.1",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.0",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-react": "^7.1.0",
    "eslint-plugin-standard": "^3.0.1",
    "expect": "^1.20.2",
    "extract-text-webpack-plugin": "^3.0.0-beta.3",
    "file-loader": "^0.11.2",
    "html-webpack-plugin": "^2.28.0",
    "image-webpack-loader": "^3.2.0",
    "mocha": "^3.3.0",
    "optimize-css-assets-webpack-plugin": "^2.0.0",
    "react-addons-test-utils": "^15.5.1",
    "style-loader": "^0.18.2",
    "url-loader": "^0.5.8",
    "webpack": "^3.0.0",
    "webpack-dev-middleware": "^1.10.1",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-manifest-plugin": "^1.1.0"
  }
}
