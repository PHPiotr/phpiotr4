extends ../layout
block content

    h1 Users

    ul(class="list-group")
        each user in users
            li(class="list-group-item")
                a(href="/users/" + encodeURIComponent(user.username))= user.full_name
    div
        if page > 0
            a(href="?page=" + (page - 1)) Previous
        if !lastPage
            a(href="?page=" + (page + 1)) Next