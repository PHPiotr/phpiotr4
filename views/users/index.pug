extends ../layout
block content

    h1 Users
    p
        a(href="/users/new") Create new profile
    ul
        each user in users
            li
                a(href="/users/" + encodeURIComponent(user.username))= user.email
    if page > 0
        a(href="?page=" + (page - 1)) Previous
    if !lastPage
        a(href="?page=" + (page + 1)) Next